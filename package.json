{
  "name": "job-tasker-v2",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start -p 3000",
    "dev": "next dev",
    "cypress:run": "cypress run --config video=false --headless",
    "cypress:open": "cypress open",
    "test:ci": "bash ./scripts/test-ci.sh",
    "start:ci": "next start -p 3000",
    "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon --typescript public/icons",
    "build:analyze": "ANALYZE=true npm run build",
    "lint": "eslint \"./cypress/**/*.{ts,tsx,js,jsx}\" --config ./cypress/eslint.config.js --fix && eslint \"{src,pages,test}/**/*.{ts,tsx,js,jsx}\" --fix",
    "lint:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:check": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "jest -c test/jest.config.cjs --passWithNoTests"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^10.0.5",
    "axios": "^0.21.1",
    "eslint-plugin-cypress": "^4.1.0",
    "next": "^12.1.6",
    "next-compose-plugins": "^2.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.5.16",
    "react-use": "^15.3.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@next/eslint-plugin-next": "^15.1.6",
    "@svgr/cli": "^5.5.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^5.0.0",
    "@types/cypress": "^0.1.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^14.18.63",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cypress": "^14.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.14.0",
    "husky": "^4.3.7",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "nock": "^13.0.6",
    "postcss": "^8.5.1",
    "prettier": "^3.2.4",
    "start-server-and-test": "^1.11.0",
    "tailwindcss": "^4.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "typescript-eslint": "^7.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint -- --quiet --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
