{
  "name": "job-tasker-v2",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start -p 3000",
    "dev": "next dev",
    "cypress:run": "cypress run --config video=false --headless",
    "cypress:open": "cypress open",
    "test": "npm run format && npm run prettier && npm run lint && npm run typecheck",
    "test:ci": "bash ./scripts/test-ci.sh",
    "start:ci": "next start -p 3000",
    "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon --typescript public/icons",
    "lint": "eslint \"{src,pages,test}/**/*.{ts,tsx,js,jsx}\" --fix",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.6.3",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "isomorphic-unfetch": "^4.0.2",
    "jest-dom": "^4.0.0",
    "next": "^15.1.6",
    "next-compose-plugins": "^2.2.1",
    "plato": "^1.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-use": "^17.6.0",
    "sass": "^1.83.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@next/bundle-analyzer": "^15.1.6",
    "@next/eslint-plugin-next": "^15.1.6",
    "@svgr/cli": "^8.1.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/cypress": "^0.1.6",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cypress": "^14.0.1",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^15.4.3",
    "nock": "^14.0.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "start-server-and-test": "^2.0.10",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
